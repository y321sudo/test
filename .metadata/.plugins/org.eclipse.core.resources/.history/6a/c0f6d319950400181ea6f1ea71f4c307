
public class Human {
//フィールド
	private String name=null;
	private int gender='0';    //性別(1:男 2:女)

//コンストラクタ
	Human(String m){
		this.name=m;
	}

	//以下メソッド
	public String getName(){
		return name;
	}
	public void setName(String name){
		this.name=name;
	}

	public int gender(){
		return gender;
	}
	public void setGender(int gender){
		this.gender=gender;
	}








}






